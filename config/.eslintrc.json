{
  "parser": "../node_modules/babel-eslint",
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": false
    },
    "sourceType": "module"
  },
  "rules": {
    "no-const-assign": 2,
    "no-extra-semi": 1,
    "no-eval": 2,
    "no-implied-eval": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 1,
    "no-redeclare": 2,
    "no-throw-literal": 2,
    "no-useless-concat": 2,
    "no-underscore-dangle": 2,
    "no-this-before-super": 1,
    "no-undef": 1,
    "no-unreachable": 1,
    "no-unused-vars": 1,
    "constructor-super": 1,
    "valid-typeof": 1,
    "prefer-const": 1,
    "radix": 2,
    "semi": 2
  }
}
