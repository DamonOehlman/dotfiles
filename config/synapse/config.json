{
  "plugins" : {
    "opensearch" : {
      "search-engines" : [
        "~/.config/synapse/search/devdocs.xml"
      ]
    }
  },
  "ui" : {
    "widgets" : {
      "extended-info-enabled" : false
    },
    "global" : {
      "ui-type" : "doish",
      "show-indicator" : false
    },
    "shortcuts" : {
      "activate" : "<Alt>grave",
      "execute" : "Return",
      "execute-without-hide" : "<Shift>Return",
      "delete-char" : "BackSpace",
      "alternative-delete-char" : "Delete",
      "next-match" : "Down",
      "prev-match" : "Up",
      "first-match" : "Home",
      "last-match" : "End",
      "next-match-page" : "Page_Down",
      "prev-match-page" : "Page_Up",
      "next-category" : "Right",
      "prev-category" : "Left",
      "next-search-type" : "Tab",
      "prev-search-type" : "<Shift>ISO_Left_Tab",
      "cancel" : "Escape",
      "paste" : "<Control>v",
      "alt-paste" : "<Shift>Insert",
      "exit" : "<Control>q"
    }
  },
  "data-sink" : {
    "global" : {
      "disabled-plugins" : [
        "SynapseBansheeActions",
        "SynapseRhythmboxActions",
        "SynapseDevhelpPlugin",
        "SynapseDictionaryPlugin",
        "SynapseDirectoryPlugin",
        "SynapseFileOpPlugin",
        "SynapseGnomeSessionPlugin",
        "SynapseLaunchpadPlugin",
        "SynapsePurplePlugin",
        "SynapseLocatePlugin",
        "SynapseChatActions"
      ]
    }
  }
}