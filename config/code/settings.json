{
  "editor.fontFamily": "Fira Code",
  "editor.fontSize": 13,
  "editor.fontLigatures": true,
  "editor.lineHeight": 20,
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "editor.renderWhitespace": "all",
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/target": true
  },
  "editor.formatOnSave": true,
  // typescript
  // "typescript.tsdk": "/Users/damo/.nvm/versions/node/v7.10.0/bin",
  "typescript.validate.enable": true,
  "javascript.validate.enable": false,
  // extensions
  "extensions.autoUpdate": true,
  "window.zoomLevel": 0,
  "gitProjectManager.baseProjectsFolders": [
    "~/code/"
  ],
  "gitProjectManager.ignoredFolders": [
    "offline",
    "node_modules"
  ],
  "gitProjectManager.maxDepthRecursion": 3,
  "gitProjectManager.checkRemoteOrigin": false,
  "gitProjectManager.storeRepositoriesBetweenSessions": true,
  "eslint.run": "onSave",
  "eslint.validate": [
    "javascript"
  ],
  "eslint.packageManager": "yarn",
  "workbench.colorTheme": "Solarized Dark",
  "workbench.iconTheme": "vs-seti",
  "files.associations": {
    ".bithoundrc": "json"
  },
  "trailing-spaces.includeEmptyLines": false,
  "flow.runOnEdit": false,
  // "flow.useNPMPackagedFlow": true,
  "extensions.ignoreRecommendations": false,
  "jsDocCompletion.enabled": false,
  "explorer.confirmDelete": false,
  "rust.rustup": {
    "toolchain": "stable-x86_64-apple-darwin",
    "nightlyToolchain": "nightly-x86_64-apple-darwin"
  },
  "rust.mode": null,
  "prettier.requireConfig": true,
  "editor.rulers": [
    80,
    120
  ],
  "[plaintext]": {
    "editor.fontLigatures": false,
  },
  "javascript.preferences.quoteStyle": "single",
  "typescript.autoImportSuggestions.enabled": false,
  "purescript.autocompleteAddImport": false,
}